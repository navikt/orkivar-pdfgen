<table>
    <tr>{{ navn }}</tr>
    <tr>Fn.nr: {{ fnr }}</tr>
</table>

<p class="margin-top:4px ">Dette dokumentet viser et øyeblikksbilde av hvordan aktivitetsplanen og dialogen var {{ journalfoeringstidspunkt }}.</p>

{{#if aktiviteter.Forslag}}<h3>Foreslåtte aktiviteter</h3>
{{#each aktiviteter.Forslag}}
    <ul>
        {{> dab/aktivitetPartial this}}
    </ul>
{{/each}}
{{/if}}

{{#if aktiviteter.Planlagt}}<h3>Planlagte aktiviteter</h3>
{{#each aktiviteter.Planlagt}}
    <ul>
        {{> dab/aktivitetPartial this}}
    </ul>
{{/each}}
{{/if}}

{{#if aktiviteter.Gjennomføres}}<h3>Aktiviteter som gjennomføres nå</h3>
{{#each aktiviteter.Gjennomføres}}
    <ul>
        {{> dab/aktivitetPartial this}}
    </ul>
{{/each}}
{{/if}}

{{#if aktiviteter.Fullført}}<h3>Fullførte aktiviteter</h3>
{{#each aktiviteter.Fullført}}
    <ul>
        {{> dab/aktivitetPartial this}}
    </ul>
{{/each}}
{{/if}}

{{#if aktiviteter.Avbrutt}}<h3>Avbrutte aktiviteter</h3>
{{#each aktiviteter.Avbrutt}}
    <ul>
        {{> dab/aktivitetPartial this}}
    </ul>
{{/each}}
{{/if}}

<h4>Dialoger</h4>
{{#each dialogtråder}}
    <div class="dialogtråd">
        <h4>{{this.overskrift}}</h4>
            {{> dab/meldingerPartial this}}
    </div>
{{/each}}
</body>
</html>
