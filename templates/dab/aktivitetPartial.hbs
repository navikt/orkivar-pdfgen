<li class="aktivitet">
    <div class="etiketter">
        {{#each this.etiketter}}
            <div class="etikett etikett-{{this.stil}}">
                {{ this.tekst }}
            </div>
        {{/each}}
    </div>
    <div class="type">{{this.type}}</div>
    <h2 class="tittel">{{this.tittel}}</h2>
    <div class="detaljer">
        {{#each this.detaljer}}
            {{#eq this.stil "LENKE"}}
                <div class="HEL_LINJE detalj">
                    <div class="detaljtittel">{{this.tittel}}</div>
                    <div class="LENKE detaljtekst">{{this.tekst}}</div>
                </div>
            {{/eq}}

            {{#not_eq this.stil "LENKE"}}
                <div class="{{this.stil}} detalj">
                    <div class="detaljtittel">{{this.tittel}}</div>
                    <div>{{breaklines this.tekst}}</div>
                </div>
            {{/not_eq}}
        {{/each}}
    </div>
    {{#each this.eksterneHandlinger}}
        <div class="eksternLenke">
            <div class="eksternlenkeTekst">TODO Bruk tekst fra var</div>
            <svg class="eksternlenkeIkon" width="1em" height="1em" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" focusable="false" role="img"><path fill-rule="evenodd" clip-rule="evenodd" d="M9.47 5.97a.75.75 0 0 1 1.06 0l5.5 5.5a.75.75 0 0 1 0 1.06l-5.5 5.5a.75.75 0 1 1-1.06-1.06L14.44 12 9.47 7.03a.75.75 0 0 1 0-1.06Z" fill="currentColor"></path></svg>
        </div>
    {{/each}}
    {{#if this.meldinger}}
        <div class="meldinger">
            <div class="dialogOverskrift"><svg class="dialogIkon" width="1em" height="1em" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" focusable="false" role="img"><path fill-rule="evenodd" clip-rule="evenodd" d="M4.75 5A.25.25 0 0 1 5 4.75h11a.25.25 0 0 1 .25.25v.5a.75.75 0 0 0 1.5 0V5A1.75 1.75 0 0 0 16 3.25H5A1.75 1.75 0 0 0 3.25 5v10a.75.75 0 0 0 1.085.67l1.502-.75a.75.75 0 0 0-.671-1.341l-.416.207V5ZM9 8.75a.25.25 0 0 0-.25.25v8c0 .138.112.25.25.25h7a.75.75 0 0 1 .335.08l2.915 1.457V9a.25.25 0 0 0-.25-.25H9ZM7.25 9c0-.966.784-1.75 1.75-1.75h10c.966 0 1.75.784 1.75 1.75v11a.75.75 0 0 1-1.085.67l-3.842-1.92H9A1.75 1.75 0 0 1 7.25 17V9Zm3 4a.75.75 0 0 1 .75-.75h.01a.75.75 0 0 1 0 1.5H11a.75.75 0 0 1-.75-.75Zm3.75-.75a.75.75 0 0 0 0 1.5h.01a.75.75 0 0 0 0-1.5H14Zm2.25.75a.75.75 0 0 1 .75-.75h.01a.75.75 0 0 1 0 1.5H17a.75.75 0 0 1-.75-.75Z" fill="currentColor"></path></svg>
                <h5 class="dialogTittel">Dialog</h5></div>
        {{> dab/meldingerPartial this}}
        </div>
    {{/if}}
</li>
