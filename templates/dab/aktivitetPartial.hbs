<li class="aktivitet">
    <div class="etiketter">
        {{#each this.etiketter}}
            <div class="etikett etikett-{{this.stil}}">
                {{ this.tekst }}
            </div>
        {{/each}}
    </div>
    <div class="type">{{this.type}}</div>
    <h2 class="tittel">{{this.tittel}}</h2>
    <div class="detaljer">
        {{#each this.detaljer}}
            {{#eq this.stil "LENKE"}}
                <div class="HEL_LINJE detalj">
                    <div class="detaljtittel">{{this.tittel}}</div>
                    <div class="LENKE detaljtekst">{{this.tekst}}</div>
                </div>
            {{/eq}}

            {{#not_eq this.stil "LENKE"}}
                <div class="{{this.stil}} detalj">
                    <div class="detaljtittel">{{this.tittel}}</div>
                    <div>{{breaklines this.tekst}}</div>
                </div>
            {{/not_eq}}
        {{/each}}
    </div>
    {{#each this.eksterneHandlinger}}
        <div class="eksternLenke">
            <div class="eksternlenkeTekst">
            <div class="eksternlenkeTekstTittel">{{this.tekst}}</div>
            {{#if this.subtekst}}
                <div class="eksternlenkeSubTekst">{{this.subtekst}}</div>
            {{/if}}
        </div>
            <svg class="eksternlenkeIkon" width="1em" height="1em" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" focusable="false" role="img"><path fill-rule="evenodd" clip-rule="evenodd" d="M9.47 5.97a.75.75 0 0 1 1.06 0l5.5 5.5a.75.75 0 0 1 0 1.06l-5.5 5.5a.75.75 0 1 1-1.06-1.06L14.44 12 9.47 7.03a.75.75 0 0 1 0-1.06Z" fill="currentColor"></path></svg>
        </div>
    {{/each}}
    {{#if this.dialogtråd}}
        <div class="meldinger">
            <div class="dialogOverskrift"><svg class="dialogIkon" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                <path fill-rule="evenodd" clip-rule="evenodd" d="M4.75 6C4.75 5.30964 5.30964 4.75 6 4.75H18C18.6904 4.75 19.25 5.30964 19.25 6V15C19.25 15.6904 18.6904 16.25 18 16.25H9C8.86407 16.25 8.73069 16.2869 8.61413 16.3569L4.75 18.6754V6ZM6 3.25C4.48122 3.25 3.25 4.48122 3.25 6V20C3.25 20.2702 3.39534 20.5195 3.63048 20.6527C3.86561 20.7858 4.15417 20.7821 4.38587 20.6431L9.20774 17.75H18C19.5188 17.75 20.75 16.5188 20.75 15V6C20.75 4.48122 19.5188 3.25 18 3.25H6ZM9 9.75C8.58579 9.75 8.25 10.0858 8.25 10.5C8.25 10.9142 8.58579 11.25 9 11.25H9.01C9.42421 11.25 9.76 10.9142 9.76 10.5C9.76 10.0858 9.42421 9.75 9.01 9.75H9ZM11.25 10.5C11.25 10.0858 11.5858 9.75 12 9.75H12.01C12.4242 9.75 12.76 10.0858 12.76 10.5C12.76 10.9142 12.4242 11.25 12.01 11.25H12C11.5858 11.25 11.25 10.9142 11.25 10.5ZM15 9.75C14.5858 9.75 14.25 10.0858 14.25 10.5C14.25 10.9142 14.5858 11.25 15 11.25H15.01C15.4242 11.25 15.76 10.9142 15.76 10.5C15.76 10.0858 15.4242 9.75 15.01 9.75H15Z" fill="#23262A"/>
